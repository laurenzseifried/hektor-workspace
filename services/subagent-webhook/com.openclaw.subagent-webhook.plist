<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.openclaw.subagent-webhook</string>
    <key>ProgramArguments</key>
    <array>
        <string>/opt/homebrew/bin/node</string>
        <string>/Users/laurenz/.openclaw/workspace/services/subagent-webhook/server.js</string>
        <string>--telegram</string>
    </array>
    <key>EnvironmentVariables</key>
    <dict>
        <key>WEBHOOK_PORT</key>
        <string>3001</string>
        <key>TELEGRAM_NOTIFY</key>
        <string>1</string>
    </dict>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/Users/laurenz/.openclaw/logs/subagent-webhook.log</string>
    <key>StandardErrorPath</key>
    <string>/Users/laurenz/.openclaw/logs/subagent-webhook.err</string>
</dict>
</plist>
